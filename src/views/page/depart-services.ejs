                                                                        <input type="hidden" class="bytes" value="<%= elem.bytes %>">
                                                                        <input type="hidden" class="format" value="<%= elem.format %>">
                                                                    </div>
                                                                </div>
                                                        </div>    
                                                    
                                                    <% }); %>
    
                                                </div>
                                                    
                                        </div>
    
    
                                        <div class="container-fluid borderView bg-secondaryLight">
                                            <div class="container-fluid  p-0 d-flex justify-content-start align-items-center flex-wrap">
                                        
                                                <div class="form-group d-flex my-2">
                                                   <span class="me-2 spnMark"><label class="me-2">Titulo : </label> <%= TitleSelect.title %></span>
                                                </div>
                                                <div class="form-group d-flex my-2">
                                                    <span class="me-2 spnMark"><label class="me-2">Categoria : </label> <%= TitleSelect.category %></span>
                                                </div>
                                                <div class="form-group d-flex my-2">
                                                    <span class="me-2 spnMark"><label class="me-2">Sub-Categoria : </label> <%= TitleSelect.sub_category %></span>
                                                </div>
                                        
                                           </div>

